<div class="listing">
  <h4><em>{{question.username}} asks: </em></h4>
  <h3>{{questionPopularity question}} {{question.title}}</h3>
  <button {{action 'addToFavorites' question}} class="button">Add to Favorites</button>
  {{show-details question=question editQuestion="editQuestion" saveAnswer="saveAnswer"}}
</div>
